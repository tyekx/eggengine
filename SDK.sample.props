    </BuildMacro>
    <BuildMacro Include="NetcodePhysXInclude">
      <Value>$(NetcodePhysXInclude)</Value>
    </BuildMacro>
    <BuildMacro Include="NetcodePhysXLibs">
      <Value>$(NetcodePhysXLibs)</Value>
    </BuildMacro>
    <BuildMacro Include="NetcodeAssimpVersion">
      <Value>$(NetcodeAssimpVersion)</Value>
    </BuildMacro>
    <BuildMacro Include="NetcodeAssimpInclude">
      <Value>$(NetcodeAssimpInclude)</Value>
    </BuildMacro>
    <BuildMacro Include="NetcodeAssimpBin">
      <Value>$(NetcodeAssimpBin)</Value>
    </BuildMacro>
    <BuildMacro Include="NetcodeAssimpLibs">
      <Value>$(NetcodeAssimpLibs)</Value>
    </BuildMacro>
    <BuildMacro Include="NetcodeLuaVersion">
      <Value>$(NetcodeLuaVersion)</Value>
    </BuildMacro>
    <BuildMacro Include="NetcodeLuaInclude">
      <Value>$(NetcodeLuaInclude)</Value>
    </BuildMacro>
    <BuildMacro Include="NetcodeLuaBin">
      <Value>$(NetcodeLuaBin)</Value>
    </BuildMacro>
    <BuildMacro Include="NetcodeLuaLibs">
      <Value>$(NetcodeLuaLibs)</Value>
    </BuildMacro>
    <BuildMacro Include="NetcodeDXTexVersion">
      <Value>$(NetcodeDXTexVersion)</Value>
    </BuildMacro>
    <BuildMacro Include="NetcodeDXTexInclude">
      <Value>$(NetcodeDXTexInclude)</Value>
    </BuildMacro>
    <BuildMacro Include="NetcodeDXTexBin">
      <Value>$(NetcodeDXTexBin)</Value>
    </BuildMacro>
    <BuildMacro Include="NetcodeDXTexLibs">
      <Value>$(NetcodeDXTexLibs)</Value>
    </BuildMacro>
    <BuildMacro Include="NetcodeSpdLogVersion">
      <Value>$(NetcodeSpdLogVersion)</Value>
    </BuildMacro>
    <BuildMacro Include="NetcodeSpdLogInclude">
      <Value>$(NetcodeSpdLogInclude)</Value>
    </BuildMacro>
    <BuildMacro Include="NetcodeSpdLogBin">
      <Value>$(NetcodeSpdLogBin)</Value>
    </BuildMacro>
    <BuildMacro Include="NetcodeSpdLogLibs">
      <Value>$(NetcodeSpdLogLibs)</Value>
    </BuildMacro>
    <BuildMacro Include="NetcodeJson11Version">
      <Value>$(NetcodeJson11Version)</Value>
    </BuildMacro>
    <BuildMacro Include="NetcodeJson11Include">
      <Value>$(NetcodeJson11Include)</Value>
    </BuildMacro>
    <BuildMacro Include="NetcodeJson11Bin">
      <Value>$(NetcodeJson11Bin)</Value>
    </BuildMacro>
    <BuildMacro Include="NetcodeJson11Libs">
      <Value>$(NetcodeJson11Libs)</Value>
    </BuildMacro>
    <BuildMacro Include="NetcodeNetcodeVersion">
      <Value>$(NetcodeNetcodeVersion)</Value>
    </BuildMacro>
    <BuildMacro Include="NetcodeNetcodeInclude">
      <Value>$(NetcodeNetcodeInclude)</Value>
    </BuildMacro>
    <BuildMacro Include="NetcodeNetcodeBin">
      <Value>$(NetcodeNetcodeBin)</Value>
    </BuildMacro>
    <BuildMacro Include="NetcodeNetcodeLibs">
      <Value>$(NetcodeNetcodeLibs)</Value>
    </BuildMacro>
    <BuildMacro Include="NetcodeBoostVersion">
