<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <NetcodeSDK>C:\sdk2</NetcodeSDK>
    <NetcodeOS>Win</NetcodeOS>
    <NetcodeRuntimeLibrary>mdd</NetcodeRuntimeLibrary>
    <NetcodePlatform>$(PlatformTarget)</NetcodePlatform>
    <NetcodeToolset>$(PlatformToolset)</NetcodeToolset>
    <NetcodeBuildType>$(Configuration)</NetcodeBuildType>
    <NetcodeBuildSelect>$(NetcodeOS)\$(NetcodePlatform)\$(NetcodeBuildType)\$(NetcodeToolSet).$(NetcodeRuntimeLibrary)</NetcodeBuildSelect>
    <NetcodeD3DLibs>d3d12.lib;d3dcompiler.lib;dxgi.lib</NetcodeD3DLibs>
    <NetcodePhysXVersion>4.1</NetcodePhysXVersion>
    <NetcodePhysXBin>$(NetcodeSDK)\PhysX\$(NetcodePhysXVersion)\$(NetcodeBuildSelect)\</NetcodePhysXBin>
    <NetcodePhysXInclude>$(NetcodeSDK)\PhysX\$(NetcodePhysXVersion)\include\</NetcodePhysXInclude>
    <NetcodePhysXLibs>PhysX_64.lib;PhysXCharacterKinematic_static_64.lib;PhysXCommon_64.lib;PhysXExtensions_static_64.lib;PhysXFoundation_64.lib;PhysXPvdSDK_static_64.lib;PhysXCooking_64.lib</NetcodePhysXLibs>
    <NetcodeAssimpVersion>5.0.0</NetcodeAssimpVersion>
    <NetcodeAssimpInclude>$(NetcodeSDK)\assimp\$(NetcodeAssimpVersion)\include\</NetcodeAssimpInclude>
    <NetcodeAssimpBin>$(NetcodeSDK)\assimp\$(NetcodeAssimpVersion)\$(NetcodeBuildSelect)\</NetcodeAssimpBin>
    <NetcodeAssimpLibs>assimp.lib</NetcodeAssimpLibs>
    <NetcodeLuaVersion>5.3.5</NetcodeLuaVersion>
    <NetcodeLuaInclude>$(NetcodeSDK)\lua\$(NetcodeLuaVersion)\include\</NetcodeLuaInclude>
    <NetcodeLuaBin>$(NetcodeSDK)\lua\$(NetcodeLuaVersion)\$(NetcodeBuildSelect)\</NetcodeLuaBin>
    <NetcodeLuaLibs>lua.lib</NetcodeLuaLibs>
    <NetcodeDXTexVersion>oct2019</NetcodeDXTexVersion>
    <NetcodeDXTexInclude>$(NetcodeSDK)\DirectXTex\$(NetcodeDXTexVersion)\include</NetcodeDXTexInclude>
    <NetcodeDXTexBin>$(NetcodeSDK)\DirectXTex\$(NetcodeDXTexVersion)\$(NetcodeBuildSelect)\</NetcodeDXTexBin>
    <NetcodeDXTexLibs>DirectXTex.lib</NetcodeDXTexLibs>
    <NetcodeSpdLogVersion>1.4.2</NetcodeSpdLogVersion>
    <NetcodeSpdLogInclude>$(NetcodeSDK)\spdlog\$(NetcodeSpdLogVersion)\include</NetcodeSpdLogInclude>
    <NetcodeSpdLogBin>$(NetcodeSDK)\spdlog\$(NetcodeSpdLogVersion)\$(NetcodeBuildSelect)\</NetcodeSpdLogBin>
    <NetcodeSpdLogLibs>spdlog.lib</NetcodeSpdLogLibs>
    <NetcodeJson11Version>1.0.0</NetcodeJson11Version>
    <NetcodeJson11Include>$(NetcodeSDK)\json11\$(NetcodeJson11Version)\include</NetcodeJson11Include>
    <NetcodeJson11Bin>$(NetcodeSDK)\json11\$(NetcodeJson11Version)\$(NetcodeBuildSelect).static\</NetcodeJson11Bin>
    <NetcodeJson11Libs>json11.lib</NetcodeJson11Libs>
    <NetcodeNetcodeVersion>0.0.0</NetcodeNetcodeVersion>
    <NetcodeNetcodeInclude>C:\sdk\netcode</NetcodeNetcodeInclude>
    <NetcodeNetcodeBin>C:\sdk\netcode\lib</NetcodeNetcodeBin>
    <NetcodeNetcodeLibs>netcode-lib.lib;netcode-protocol.lib</NetcodeNetcodeLibs>
    <NetcodeBoostVersion>1.72.0</NetcodeBoostVersion>
    <NetcodeBoostInclude>$(NetcodeSDK)\boost\$(NetcodeBoostVersion)\include</NetcodeBoostInclude>
    <NetcodeBoostBin>$(NetcodeSDK)\boost\$(NetcodeBoostVersion)\$(NetcodeBuildSelect)\</NetcodeBoostBin>
    <NetcodeBoostLibs>libboost_system-vc-mt-gd-x64-1_72.lib;libboost_regex-vc-mt-gd-x64-1_72.lib;libboost_program_options-vc-mt-gd-x64-1_72.lib;libboost_coroutine-vc-mt-gd-x64-1_72.lib;libboost_date_time-vc-mt-gd-x64-1_72.lib;libboost_context-vc-mt-gd-x64-1_72.lib</NetcodeBoostLibs>
    <NetcodeMysqlconnVersion>8.0.19</NetcodeMysqlconnVersion>
    <NetcodeMysqlconnInclude>$(NetcodeSDK)\mysqlconn\$(NetcodeMysqlconnVersion)\include</NetcodeMysqlconnInclude>
    <NetcodeMysqlconnBin>$(NetcodeSDK)\mysqlconn\$(NetcodeMysqlconnVersion)\$(NetcodeBuildSelect)\</NetcodeMysqlconnBin>
    <NetcodeMysqlconnLibs>mysqlcppconn8.lib</NetcodeMysqlconnLibs>
    <NetcodeProtobufVersion>3.11.4</NetcodeProtobufVersion>
    <NetcodeProtobufInclude>$(NetcodeSDK)\protobuf\$(NetcodeProtobufVersion)\include</NetcodeProtobufInclude>
    <NetcodeProtobufBin>$(NetcodeSDK)\protobuf\$(NetcodeProtobufVersion)\$(NetcodeBuildSelect)</NetcodeProtobufBin>
    <NetcodeProtobufLibs>libprotobufd.lib</NetcodeProtobufLibs>
    <NetcodeIncludes>$(NetcodePhysXInclude);$(NetcodeAssimpInclude);$(NetcodeLuaInclude);$(NetcodeDXTexInclude);$(NetcodeSpdLogInclude);$(NetcodeJson11Include);$(NetcodeNetcodeInclude);$(NetcodeBoostInclude);$(NetcodeMysqlconnInclude)</NetcodeIncludes>
    <NetcodeBinaries>$(NetcodePhysXBin);$(NetcodeAssimpBin);$(NetcodeLuaBin);$(NetcodeDXTexBin);$(NetcodeSpdLogBin);$(NetcodeJson11Bin);$(NetcodeNetcodeBin);$(NetcodeBoostBin);$(NetcodeMysqlconnBin);$(NetcodeProtobufBin)</NetcodeBinaries>
    <NetcodeLibraries>$(NetcodeD3DLibs);$(NetcodePhysXLibs);$(NetcodeLuaLibs);$(NetcodeAssimpLibs);$(NetcodeDXTexLibs);$(NetcodeSpdLogLibs);$(NetcodeJson11Libs);$(NetcodeNetcodeLibs);$(NetcodeBoostLibs);$(NetcodeMysqlconnLibs);$(NetcodeProtobufLibs)</NetcodeLibraries>
    <NetcodeDlls>$(NetcodePhysXBin);$(NetcodeAssimpBin);$(NetcodeLuaBin);$(NetcodeMysqlconnBin)</NetcodeDlls>
  </PropertyGroup>
  <PropertyGroup>
    <OutDir>$(SolutionDir)Bin\v142-msvc\</OutDir>
    <IntDir>$(SolutionDir)Build\v142-msvc\$(ProjectName)\</IntDir>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <AdditionalIncludeDirectories>$(NetcodeIncludes);%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
      <PreprocessorDefinitions>WIN32_LEAN_AND_MEAN;NOMINMAX;BOOST_ALL_NO_LIB;_SILENCE_CXX17_ITERATOR_BASE_CLASS_DEPRECATION_WARNING;%(PreprocessorDefinitions)</PreprocessorDefinitions>
    </ClCompile>
    <Link>
      <AdditionalLibraryDirectories>$(NetcodeBinaries);%(AdditionalLibraryDirectories)</AdditionalLibraryDirectories>
      <AdditionalDependencies>$(NetcodeLibraries);%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
    <FxCompile>
      <ShaderModel>5.0</ShaderModel>
      <ObjectFileOutput>$(OutDir)Shaders\%(Filename).cso</ObjectFileOutput>
    </FxCompile>
  </ItemDefinitionGroup>
  <ItemGroup>
    <BuildMacro Include="NetcodeSDK">
      <Value>$(NetcodeSDK)</Value>
    </BuildMacro>
    <BuildMacro Include="NetcodeOS">
      <Value>$(NetcodeOS)</Value>
    </BuildMacro>
