    </BuildMacro>
    <BuildMacro Include="NetcodeMysqlconnLibs">
      <Value>$(NetcodeMysqlconnLibs)</Value>
    </BuildMacro>
    <BuildMacro Include="NetcodeIncludes">
      <Value>$(NetcodeIncludes)</Value>
    </BuildMacro>
    <BuildMacro Include="NetcodeBinaries">
